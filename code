#include <Servo.h>

Servo servoLeft;
Servo servoRight;

void setup() {
  tone(4, 3000, 1000);
  delay(1000);
  
  servoLeft.attach(13);
  servoRight.attach(12);

  servoLeft.writeMicroseconds(1530);
  servoRight.writeMicroseconds(1300);
  delay(6400);

  servoLeft.detach();
  servoRight.detach();

}

void loop() {
  // put your main code here, to run repeatedly:

}
